{
    "name": "marcelo-bogas/plates",
    "type": "project",
    "description": "PHP project managed by composer",
    "keywords": ["php", "composer"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "config/app.php"
        ]
    },
    "require": {
        "php": "^7.4|^8.0",
        "coffeecode/router": "^2.0",
        "league/plates": "^3.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "optimize-autoloader": true
    }
}
